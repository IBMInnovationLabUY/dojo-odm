

public class Siniestro {
	/*
	 * TODOS LOS VALORES SON A MODO DE EJEMPLO.
	 * NO REPRESENTAN LA REALIDAD 100%
	 */
	private String tipo;
	private int id;

	public Siniestro() {
	}
	public String getTipoSiniestro() {
		return tipo;
	}
	public int getIdSiniestro() {
		return id;
	}
	public void setIdSiniestro(int idSiniestro) {
		this.id = idSiniestro;
	}
	public void setTipoSiniestro(String tipoSiniestro) {
		this.tipo = tipoSiniestro;
	}
	
}